  "main": "main.ts",
  "scripts": {
    "build:backend": "npm run clean:backend && webpack -c config/backend.config.js --mode production --progress",
    "build:crawler": "npm run clean:crawler && webpack -c config/crawler.config.js --mode production --progress",
    "build:frontend": "npm run clean:frontend && webpack -c config/frontend.config.js --mode production --progress",
    "clean:backend": "rimraf .out/backend",
    "clean:crawler": "rimraf .out/crawler",
    "clean:frontend": "rimraf .out/frontend",
    "commit": "cz",
    "run:crawler": "node .out/crawler/main.js",
    "serve:backend": "nodemon --config config/backend.nodemon.json .out/backend/main.js",
    "serve:crawler": "nodemon --config config/crawler.nodemon.json .out/crawler/main.js",
    "serve:frontend": "browser-sync start -c browsersync.js",
    "watch:backend": "npm run clean:backend && webpack -c config/backend.config.js -w --mode development --progress",
    "watch:crawler": "npm run clean:crawler && webpack -c config/crawler.config.js -w --mode development --progress",
    "watch:frontend": "npm run clean:frontend && webpack -c config/frontend.config.js -w --mode development --progress"
  },
